    <SolidColorBrush x:Key="SelectorThumbBrush" Color="#FFA4DFF2"/>
    <SolidColorBrush x:Key="SelectorThumbStroke" Color="#FF777F85"/>

    <Style x:Key="roundthumbstyle" TargetType="Thumb">
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="Width" Value="20"/>
        <Setter Property="Height" Value="20"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="Thumb">
                    <Grid>
                        <Grid>
                            <Ellipse Fill="{StaticResource SelectorThumbBrush}" StrokeThickness="1"/>
                            <Ellipse Margin="5" Fill="White" Stroke="{StaticResource SelectorThumbStroke}" StrokeThickness="1"/>

                        </Grid>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <!--Box Whisker Chart Tooltip Template-->
    <DataTemplate x:Key="BoxWhiskerTooltipTemplate">
        <Border  Background="Transparent" BorderBrush="Transparent"  BorderThickness="1" Padding="5">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition></RowDefinition>
                    <RowDefinition></RowDefinition>
                    <RowDefinition></RowDefinition>
                    <RowDefinition></RowDefinition>
                    <RowDefinition></RowDefinition>
                    <RowDefinition></RowDefinition>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto"></ColumnDefinition>
                    <ColumnDefinition Width="*"></ColumnDefinition>
                    <ColumnDefinition Width="*"></ColumnDefinition>
                </Grid.ColumnDefinitions>
                <TextBlock Grid.Row="0" Grid.Column="0" Text="{local:ChartLocalizationResourceExtension ResourceName=Maximum}" Foreground="{ThemeResource SyncfusionChartTooltipForeground}" FontSize="{ThemeResource SyncfusionCaptionFontSize}"/>
                <TextBlock Grid.Row="0" Grid.Column="1" Text=" :" FontSize="{ThemeResource SyncfusionCaptionFontSize}" Foreground="{ThemeResource SyncfusionChartTooltipForeground}"></TextBlock>
                <TextBlock Margin="2,0,0,0" Grid.Row="0" Grid.Column="2" Text="{Binding Maximum}" Foreground="{ThemeResource SyncfusionChartTooltipForeground}" FontSize="{ThemeResource SyncfusionCaptionFontSize}" />

                <TextBlock Grid.Row="1" Grid.Column="0"  Text="{local:ChartLocalizationResourceExtension ResourceName=Q3}" Foreground="{ThemeResource SyncfusionChartTooltipForeground}" FontSize="{ThemeResource SyncfusionCaptionFontSize}"/>
                <TextBlock Grid.Row="1" Grid.Column="1" Text=" :" FontSize="{ThemeResource SyncfusionCaptionFontSize}" Foreground="{ThemeResource SyncfusionChartTooltipForeground}"></TextBlock>
                <TextBlock Margin="2,0,0,0" Grid.Row="1" Grid.Column="2" Text="{Binding UppperQuartile}" Foreground="{ThemeResource SyncfusionChartTooltipForeground}" FontSize="{ThemeResource SyncfusionCaptionFontSize}" />

                <TextBlock Grid.Row="2" Grid.Column="0"  Text="{local:ChartLocalizationResourceExtension ResourceName=Median}" Foreground="{ThemeResource SyncfusionChartTooltipForeground}" FontSize="{ThemeResource SyncfusionCaptionFontSize}"/>
                <TextBlock Grid.Row="2" Grid.Column="1" Text=" :" FontSize="{ThemeResource SyncfusionCaptionFontSize}" Foreground="{ThemeResource SyncfusionChartTooltipForeground}"></TextBlock>
                <TextBlock Margin="2,0,0,0" Grid.Row="2" Grid.Column="2" Text="{Binding Median}" Foreground="{ThemeResource SyncfusionChartTooltipForeground}" FontSize="{ThemeResource SyncfusionCaptionFontSize}" />

                <TextBlock Grid.Row="3" Grid.Column="0" Text="{local:ChartLocalizationResourceExtension ResourceName=Q1}" Foreground="{ThemeResource SyncfusionChartTooltipForeground}" FontSize="{ThemeResource SyncfusionCaptionFontSize}"/>
                <TextBlock Grid.Row="3" Grid.Column="1" Text=" :" FontSize="{ThemeResource SyncfusionCaptionFontSize}" Foreground="{ThemeResource SyncfusionChartTooltipForeground}"></TextBlock>
                <TextBlock Margin="2,0,0,0" Grid.Row="3" Grid.Column="2" Text="{Binding LowerQuartile}" Foreground="{ThemeResource SyncfusionChartTooltipForeground}" FontSize="{ThemeResource SyncfusionCaptionFontSize}" />

                <TextBlock Grid.Row="4" Grid.Column="0" Text="{local:ChartLocalizationResourceExtension ResourceName=Minimum}" Foreground="{ThemeResource SyncfusionChartTooltipForeground}" FontSize="{ThemeResource SyncfusionCaptionFontSize}"/>
                <TextBlock Grid.Row="4" Grid.Column="1" Text=" :" FontSize="{ThemeResource SyncfusionCaptionFontSize}" Foreground="{ThemeResource SyncfusionChartTooltipForeground}"></TextBlock>
                <TextBlock Margin="2,0,0,0" Grid.Row="4" Grid.Column="2" Text="{Binding Minimum}" Foreground="{ThemeResource SyncfusionChartTooltipForeground}" FontSize="{ThemeResource SyncfusionCaptionFontSize}" />
            </Grid>
        </Border>
    </DataTemplate>

    <!--Financial Chart Tooltip Template-->
    <DataTemplate x:Key="FinancialTooltipTemplate">
        <Border  Background="Transparent" BorderBrush="Transparent" Padding="5">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition></RowDefinition>
                    <RowDefinition></RowDefinition>
                    <RowDefinition></RowDefinition>
                    <RowDefinition></RowDefinition>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto"></ColumnDefinition>
                    <ColumnDefinition Width="*"></ColumnDefinition>
                    <ColumnDefinition Width="*"></ColumnDefinition>
                </Grid.ColumnDefinitions>
                <TextBlock Grid.Row="0" Grid.Column="0"  Text="{local:ChartLocalizationResourceExtension ResourceName=Open}" Foreground="{ThemeResource SyncfusionChartTooltipForeground}" FontSize="{ThemeResource SyncfusionCaptionFontSize}"/>
                <TextBlock Grid.Row="0" Grid.Column="1" Text=" :" FontSize="{ThemeResource SyncfusionCaptionFontSize}" Foreground="{ThemeResource SyncfusionChartTooltipForeground}"></TextBlock>
                <TextBlock Margin="2,0,0,0" Grid.Row="0" Grid.Column="2" Text="{Binding Open}" Foreground="{ThemeResource SyncfusionChartTooltipForeground}" FontSize="{ThemeResource SyncfusionCaptionFontSize}" />

                <TextBlock Grid.Row="1" Grid.Column="0" Text="{local:ChartLocalizationResourceExtension ResourceName=High}"  Foreground="{ThemeResource SyncfusionChartTooltipForeground}" FontSize="{ThemeResource SyncfusionCaptionFontSize}"/>
                <TextBlock Grid.Row="1" Grid.Column="1" Text=" :" FontSize="{ThemeResource SyncfusionCaptionFontSize}" Foreground="{ThemeResource SyncfusionChartTooltipForeground}"></TextBlock>
                <TextBlock Margin="2,0,0,0" Grid.Row="1" Grid.Column="2" Text="{Binding High}" Foreground="{ThemeResource SyncfusionChartTooltipForeground}" FontSize="{ThemeResource SyncfusionCaptionFontSize}" />

                <TextBlock Grid.Row="2" Grid.Column="0" Text="{local:ChartLocalizationResourceExtension ResourceName=Low}"  Foreground="{ThemeResource SyncfusionChartTooltipForeground}" FontSize="{ThemeResource SyncfusionCaptionFontSize}"/>
                <TextBlock Grid.Row="2" Grid.Column="1" Text=" :" FontSize="{ThemeResource SyncfusionCaptionFontSize}" Foreground="{ThemeResource SyncfusionChartTooltipForeground}"></TextBlock>
                <TextBlock Margin="2,0,0,0" Grid.Row="2" Grid.Column="2" Text="{Binding Low}" Foreground="{ThemeResource SyncfusionChartTooltipForeground}" FontSize="{ThemeResource SyncfusionCaptionFontSize}" />

                <TextBlock Grid.Row="3" Grid.Column="0" Text="{local:ChartLocalizationResourceExtension ResourceName=Close}" Foreground="{ThemeResource SyncfusionChartTooltipForeground}" FontSize="{ThemeResource SyncfusionCaptionFontSize}"/>
                <TextBlock Grid.Row="3" Grid.Column="1" Text=" :" FontSize="{ThemeResource SyncfusionCaptionFontSize}" Foreground="{ThemeResource SyncfusionChartTooltipForeground}"></TextBlock>
                <TextBlock Margin="2,0,0,0" Grid.Row="3" Grid.Column="2" Text="{Binding Close}" Foreground="{ThemeResource SyncfusionChartTooltipForeground}" FontSize="{ThemeResource SyncfusionCaptionFontSize}" />
            </Grid>
        </Border>
    </DataTemplate>

    <!--Range type series Tooltip Template-->
    <DataTemplate x:Key="RangeTooltipTemplate">
        <Border Background="Transparent" BorderBrush="Transparent" Padding="5">

            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition></RowDefinition>
                    <RowDefinition></RowDefinition>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto"></ColumnDefinition>
                    <ColumnDefinition Width="*"></ColumnDefinition>
                    <ColumnDefinition Width="*"></ColumnDefinition>
                </Grid.ColumnDefinitions>
                <TextBlock Grid.Row="0" Grid.Column="0" Text="{local:ChartLocalizationResourceExtension ResourceName=High}" Foreground="{ThemeResource SyncfusionChartTooltipForeground}" FontSize="{ThemeResource SyncfusionCaptionFontSize}"/>
                <TextBlock Grid.Row="0" Grid.Column="1" Text=" :" FontSize="{ThemeResource SyncfusionCaptionFontSize}" Foreground="{ThemeResource SyncfusionChartTooltipForeground}"></TextBlock>
                <TextBlock Grid.Row="0" Grid.Column="2" Margin="2,0,0,0" Text="{Binding High}" Foreground="{ThemeResource SyncfusionChartTooltipForeground}" FontSize="{ThemeResource SyncfusionCaptionFontSize}" />

                <TextBlock Grid.Row="1" Grid.Column="0" Text="{local:ChartLocalizationResourceExtension ResourceName=Low}" Foreground="{ThemeResource SyncfusionChartTooltipForeground}" FontSize="{ThemeResource SyncfusionCaptionFontSize}"/>
                <TextBlock Grid.Row="1" Grid.Column="1" Text=" :" FontSize="{ThemeResource SyncfusionCaptionFontSize}" Foreground="{ThemeResource SyncfusionChartTooltipForeground}"></TextBlock>
                <TextBlock Grid.Row="1" Grid.Column="2" Margin="2,0,0,0" Text="{Binding Low}" Foreground="{ThemeResource SyncfusionChartTooltipForeground}" FontSize="{ThemeResource SyncfusionCaptionFontSize}" />
            </Grid>
        </Border>
    </DataTemplate>

    <DataTemplate x:Key="leftTooltipTemplate">
        <Grid>
            <Path x:Name="light_left" Data="F1M398.0605,350.6934L377.7585,350.6934L372.3835,350.6934L372.3215,350.6934C370.2155,350.6934,368.5085,352.5044,368.5085,354.7344C368.5085,356.9664,370.2155,358.7774,372.3215,358.7774L372.3835,358.7774L377.7585,358.7774L398.0605,358.7774z" Fill="{ThemeResource SyncfusionChartTooltipFill}" Stretch="Fill"/>
            <TextBlock Text="{Binding}" Margin="5,5,5,5" Foreground="{ThemeResource SyncfusionChartTooltipLabelForeground}"/>
        </Grid>
    </DataTemplate>

    <DataTemplate x:Key="rightTooltipTemplate">
        <Grid>
            <Path x:Name="dark_right" Data="F1M400.5342,362.2568L420.8372,362.2568L426.2122,362.2568L426.2742,362.2568C428.3802,362.2568,430.0872,364.0658,430.0872,366.2978C430.0872,368.5298,428.3802,370.3388,426.2742,370.3388L426.2122,370.3388L420.8372,370.3388L400.5342,370.3388z" Fill="{ThemeResource SyncfusionChartTooltipFill}" Stretch="Fill" />
            <TextBlock Text="{Binding}" Margin="5,5,5,5" Foreground="{ThemeResource SyncfusionChartTooltipLabelForeground}"/>
        </Grid>
    </DataTemplate>

    <!--Line Series default tooltip template-->
    <DataTemplate x:Key="LineTooltipTemplate">
        <Border Background="{ThemeResource SyncfusionChartTooltipFill}" BorderBrush="{ThemeResource SyncfusionChartTooltipStroke}"  BorderThickness="1"  Padding="5">
            <TextBlock Text="{Binding YData}" Foreground="{ThemeResource SyncfusionChartTooltipForeground}" FontSize="{ThemeResource SyncfusionCaptionFontSize}" />
        </Border>
    </DataTemplate>

    <DataTemplate x:Key="AreaTooltipTemplate">
        <Border Background="{ThemeResource SyncfusionChartTooltipFill}" BorderBrush="{ThemeResource SyncfusionChartTooltipStroke}"  BorderThickness="1" Padding="5">
            <TextBlock Text="{Binding YData}" Foreground="{ThemeResource SyncfusionChartTooltipForeground}" FontSize="{ThemeResource SyncfusionCaptionFontSize}" />
        </Border>
    </DataTemplate>

    <SolidColorBrush x:Key="AxisLabelBrush" Color="#1E90FF"/>

    <DataTemplate x:Key="AxisLabel">
        <Border  CornerRadius="2">
            <Grid Background="{StaticResource AxisLabelBrush}" MinWidth="28"  MinHeight="20">
                <TextBlock Text="{Binding}"  Padding="2" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="White"></TextBlock>
            </Grid>
        </Border>
    </DataTemplate>

    <DataTemplate x:Key="textBlockAnnotation">
        <TextBlock Foreground="{ThemeResource SyncfusionChartAnnotationForeground}"/>
    </DataTemplate>

    <DataTemplate x:Key="textBoxAnnotation">
        <TextBox AcceptsReturn="True" Foreground="{ThemeResource SyncfusionChartAnnotationForeground}"/>
    </DataTemplate>

    <DataTemplate x:Key="AnnotationTooltipTemplate">
        <Border CornerRadius="5" Background="White"  BorderThickness="1" Padding="4" >
            <TextBlock Text="{Binding}" Foreground="Black" FontSize="{ThemeResource SyncfusionCaptionFontSize}" />
        </Border>
    </DataTemplate>

    <Style TargetType="Line" x:Key="defaultWaterfallConnectorStyle">
        <Setter Property="Stroke" Value="Black"/>
        <Setter Property="StrokeThickness" Value="0.5"/>
    </Style>

    <Style TargetType="Polyline" x:Key="defaultHistogramSeriesCurveLineStyle">
        <Setter Property="Stroke" Value="Black"/>
        <Setter Property="StrokeThickness" Value="1"/>
    </Style>

    <Style x:Key="lineStyle" TargetType="Line">
        <Setter Property="Stroke" Value="{ThemeResource ApplicationForegroundThemeBrush}"></Setter>
        <Setter Property="StrokeThickness" Value="1"></Setter>
    </Style>
    
    <DataTemplate x:Key="trackerSymbolTemplate">
        <Ellipse Fill="{Binding Fill}" Stroke="{Binding Stroke}" StrokeThickness="{Binding StrokeThickness}" Height="{Binding Height}" Width="{Binding Width}">
        </Ellipse>
    </DataTemplate>

</ResourceDictionary>
