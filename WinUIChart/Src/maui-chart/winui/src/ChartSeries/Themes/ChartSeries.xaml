    <Style x:Key="ErrorBarSeriesStyle" TargetType="local:ErrorBarSeries">
        <Setter Property="VisibilityOnLegend" Value="Collapsed"></Setter>
        <Setter Property="HorizontalLineStyle">
            <Setter.Value>
                <local:LineStyle></local:LineStyle>
            </Setter.Value>
        </Setter>
        <Setter Property="HorizontalCapLineStyle">
            <Setter.Value>
               <local:CapLineStyle></local:CapLineStyle>
            </Setter.Value>
        </Setter>
        <Setter Property="VerticalLineStyle">
            <Setter.Value>
                <local:LineStyle></local:LineStyle>
            </Setter.Value>
        </Setter>
        <Setter Property="VerticalCapLineStyle">
            <Setter.Value>
                <local:CapLineStyle></local:CapLineStyle>
            </Setter.Value>
        </Setter>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="local:ErrorBarSeries">
                       <local:ChartSeriesPanel x:Name="SyncfusionChartSeriesPanel"></local:ChartSeriesPanel>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

